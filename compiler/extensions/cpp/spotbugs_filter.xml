        <And>
            <Class name="zserio.extension.cpp.types.NativeArrayTraits"/>
            <Field name="$SWITCH_TABLE$zserio$extension$cpp$types$NativeArrayTraits$TYPE"/>
        </And>
    </Match>
</FindBugsFilter>
